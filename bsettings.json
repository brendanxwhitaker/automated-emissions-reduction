{
    "1": "====================ENVIRONMENT====================",
    "2": "====================TRAINER====================",

    "time_steps": 10000000,
    "reuse_state_dicts": true,
    "policy_score_frequency": 8,
    "ema_alpha": 0.9,
    "greedy_temperature": 1e-4,
    "mp": false,

    "6": "====================POLICY====================",

    "algo": "ppo",
    "lr": 3e-4,
    "min_lr": 5e-6,
    "eps": 1e-5,
    "alpha": 1e-5,
    "gamma": 0.99,
    "use_gae": false,
    "gae_lambda": 0.95,
    "entropy_coef": 0.01,
    "value_loss_coef": 1.0,
    "max_grad_norm": 0.5,
    "seed": 2,
    "cuda_deterministic": false,
    "num_processes": 1,
    "num_steps": 2048,
    "ppo_epoch": 4,
    "num_mini_batch": 1,
    "clip_param": 0.3,
    "log_interval": 10,
    "save_interval": 2048,
    "env_name": "bees",
    "log_dir": "/tmp/gym/",
    "cuda": false,
    "use_proper_time_limits": false,
    "base_kwargs": {
        "recurrent": false,
        "hidden_size": 128,
        "num_layers": 4
    },
    "use_linear_lr_decay": false,
    "use_clipped_value_loss": true,

    "4": "============ONECYCLELR===============",
    "lr_cycle_steps": 200,
    "pct_start": 0.3
}
